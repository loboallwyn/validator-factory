        "illuminate/translation": "^10.43.0",
        "illuminate/validation": "^10.43.0"
    },
    "require-dev": {
        "orchestra/testbench": "^8.21.1"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Jefferson Ochoa (Jeff)"
        },
        {
            "name": "Allwyn Lobo (Allwyn)"
        }
    ],
    "autoload": {
